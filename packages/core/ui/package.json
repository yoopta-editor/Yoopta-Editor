{
  "name": "@yoopta/ui",
  "version": "6.0.0-beta.4",
  "description": "UI components for Yoopta Editor",
  "author": "Darginec05 <devopsbanda@gmail.com>",
  "homepage": "https://github.com/Darginec05/Yoopta-Editor#readme",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "type": "module",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "toolbar": ["./dist/toolbar/index.d.ts"],
      "action-menu-list": ["./dist/action-menu-list/index.d.ts"],
      "slash-command-menu": ["./dist/slash-command-menu/index.d.ts"],
      "block-options": ["./dist/block-options/index.d.ts"],
      "floating-block-actions": ["./dist/floating-block-actions/index.d.ts"],
      "highlight-color-picker": ["./dist/highlight-color-picker/index.d.ts"],
      "portal": ["./dist/portal/index.d.ts"],
      "overlay": ["./dist/overlay/index.d.ts"],
      "theme": ["./dist/theme/index.d.ts"]
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./toolbar": {
      "types": "./dist/toolbar/index.d.ts",
      "import": "./dist/toolbar.js"
    },
    "./action-menu-list": {
      "types": "./dist/action-menu-list/index.d.ts",
      "import": "./dist/action-menu-list.js"
    },
    "./slash-command-menu": {
      "types": "./dist/slash-command-menu/index.d.ts",
      "import": "./dist/slash-command-menu.js"
    },
    "./block-options": {
      "types": "./dist/block-options/index.d.ts",
      "import": "./dist/block-options.js"
    },
    "./floating-block-actions": {
      "types": "./dist/floating-block-actions/index.d.ts",
      "import": "./dist/floating-block-actions.js"
    },
    "./highlight-color-picker": {
      "types": "./dist/highlight-color-picker/index.d.ts",
      "import": "./dist/highlight-color-picker.js"
    },
    "./portal": {
      "types": "./dist/portal/index.d.ts",
      "import": "./dist/portal.js"
    },
    "./overlay": {
      "types": "./dist/overlay/index.d.ts",
      "import": "./dist/overlay.js"
    },
    "./theme": {
      "types": "./dist/theme/index.d.ts",
      "import": "./dist/theme.js"
    }
  },
  "files": [
    "dist/"
  ],
  "peerDependencies": {
    "@yoopta/editor": ">=4.9.9",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@floating-ui/react": "^0.26.9",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.294.0",
    "react-colorful": "^5.6.1",
    "tailwind-merge": "^2.0.0",
    "use-debounce": "^10.0.0",
    "zustand": "^4.5.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Darginec05/Yoopta-Editor.git"
  },
  "scripts": {
    "test": "node ./__tests__/yoopta-ui.test.js",
    "start": "rollup --config rollup.config.js --watch --bundleConfigAsCjs --environment NODE_ENV:development",
    "prepublishOnly": "yarn build",
    "build": "rollup --config rollup.config.js --bundleConfigAsCjs --environment NODE_ENV:production"
  },
  "bugs": {
    "url": "https://github.com/Darginec05/Yoopta-Editor/issues"
  },
  "gitHead": "0c1eed956a263573e6aff9bdf58dcaf834deadf5"
}
